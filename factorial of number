# FOR VERY LARGE NUMBERS

def range_prod(lo,hi):
    if lo+1 < hi:
        mid = (hi+lo)//2
        return range_prod(lo,mid) * range_prod(mid+1,hi)
    if lo == hi:
        return lo
    return lo*hi

def treefactorial(n):
    if n < 2:
        return 1
    return range_prod(1,n)
    
--------------------------------------------------------------------------------------------------------------------
 
 def factorial(self, N):
       #code here
    fac = 1
    for i in range(1,N+1):
        fac*=i
    return fac
    
----------------------------------------------------------------------------------------------------------------------

import math
def factorial(self, N):
    k=math.factorial(N)
    
